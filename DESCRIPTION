Package: Gen3
Title: Interface to Gen3 Data Services Within AnVIL
Version: 0.0.1
Authors@R: 
    person(
        given = "Morgan",
        family = "Martin",
        role = c("aut", "cre"),
        email = "Martin.Morgan@RoswellPark.org",
        comment = c(ORCID = "0000-0002-5874-8148")
